{
  "name": "gatsby-fitness-site",
  "private": true,
  "description": "Gatsby fitness site",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "typecheck": "tsc --project . --noEmit",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@types/react": "^16.9.52",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.4",
    "babel-plugin-styled-components": "^1.11.1",
    "date-fns": "^2.16.1",
    "gatsby": "^2.27.1",
    "gatsby-image": "^2.7.0",
    "gatsby-plugin-netlify-cms": "^4.3.17",
    "gatsby-plugin-netlify-cms-paths": "^1.3.0",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sharp": "^2.10.0",
    "gatsby-plugin-sitemap": "^2.7.0",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-remark-images": "^3.7.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^2.4.2",
    "gatsby-transformer-remark": "^2.9.2",
    "gatsby-transformer-sharp": "^2.8.0",
    "lazysizes": "^5.2.2",
    "netlify-cms-app": "^2.13.3",
    "netlify-cms-media-library-cloudinary": "^1.3.8",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@graphql-codegen/cli": "1.19.1",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-operations": "1.17.9",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "apollo": "^2.31.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "2.1.2",
    "react-is": "^17.0.1",
    "typescript": "^4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}?(x)": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
